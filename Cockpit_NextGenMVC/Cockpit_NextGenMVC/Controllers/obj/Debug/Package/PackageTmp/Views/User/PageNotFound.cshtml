@{
    ViewBag.Title = "PageNotFound";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="content-wrapper">
    <section class="content">
    <div class="row">
    <div class="col-xs-12">
    <div class="box">

    <!-- Main content -->
        <section class="content">
          <div class="error-page">
            <h2 class="headline text-yellow"> 404</h2>
            <div class="error-content">
              <h3><i class="fa fa-warning text-yellow"></i> Oops!  @ViewBag.LookupPagePath  Page not found.</h3>
              <p>
                We could not find the page you were looking for.
                Meanwhile, you may <a href="@Url.Action("Index", "Home")">return to dashboard</a> or try using the search form.
              </p>
            </div><!-- /.error-content -->
          </div><!-- /.error-page -->
        </section><!-- /.content -->


        </div>
    </div>
    </div>
    </section>
</div>
